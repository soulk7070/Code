from pyngrok import ngrok

# Ganti isi string di bawah dengan token kamu
ngrok.set_auth_token("MASUKKAN_TOKEN_KAMU_DI_SINI")


# Jalankan ComfyUI
import subprocess
process = subprocess.Popen(["python", "main.py", "--dont-print-server", "--port", "8188"])

